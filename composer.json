{
	"name": "parm/parm",
	"type": "library",
	"description": "PHP Active Record for MySQL. Connects to the your database and generates the Object Relational Mapping. Built in Database Query Processor for custom queries. Ability to use closures to process rows",
	"keywords": ["active record", "activerecord", "orm", "closures", "dao", "database", "mysql"],
	"homepage": "http://github.com/cassell/parm",
	"license": "BSD-2-Clause",
	"authors": [
		{
			"name": "Andrew Cassell",
			"email": "me@andrewcassell.com",
			"homepage": "http://www.andrewcassell.com",
			"role": "Web Application Developer"
		}
	],
	"require": {
		"php": ">=5.5.0",
		"mustache/mustache": "^2.3",
		"doctrine/dbal": "^2.5"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^4.7.7",
	  	"codeclimate/php-test-reporter": "dev-master",
		"ramsey/uuid" : "^3.0.0"
	},
	"autoload": {
		"psr-4": {"Parm\\": "src/"}
	},
	"support": {
		"issues": "https://github.com/cassell/parm/issues"
	}
}
